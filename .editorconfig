root = true

[*]
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

# JavaScript
[*.js]
indent_style = space
indent_size = 4

# JSON
[*.json]
indent_style = space
indent_size = 2

# TypeScript
[*.ts]
indent_style = space
indent_size = 4
